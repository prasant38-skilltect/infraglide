{
  "terraform": {
    "required_providers": {
      "aws": {
        "source": "hashicorp/aws",
        "version": "~> 5.0"
      }
    }
  },
  "provider": {
    "aws": {
      "region": "us-east-1"
    }
  },
  "resource": {
    "aws_s3_bucket": {
      "example": {
        "bucket": "my-test-bucket-12345",
        "tags": {
          "Name": "Test Bucket",
          "Environment": "Development"
        }
      }
    }
  }
}
